program logicos{
    println true && false;         // false
    println true || false;         // true
    println !false;                // true
    println 5 > 3;                 // true
    println 2 < 1;                 // false
    println 7 == 7;                // true
    println 4 != 5;                // true
    println (3 + 2) >= 5;          // true
    println (3 * 2) <= 6;          // true
}