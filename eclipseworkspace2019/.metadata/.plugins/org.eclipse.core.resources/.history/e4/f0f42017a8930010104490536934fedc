package com.miorganizacion.simple.interprete.ast;

public class VarDecl implements ASTNode {
    private String name;
    
    public VarDecl(String name) {
        this.name = name;
    }

    @Override
    public Object execute() {
        // Necesitarás acceso al symbolTable aquí
        // Esto se puede pasar por constructor o mediante un contexto
        return null;
    }
    
    public String getName() {
        return name;
    }
}