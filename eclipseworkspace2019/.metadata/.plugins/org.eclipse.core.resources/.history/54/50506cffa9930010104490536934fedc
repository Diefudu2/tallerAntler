package com.miorganizacion.simple.interprete.ast;

public class Variable implements ASTNode {
    private String name;
    private Map<String, Object> symbolTable;
    
    public Variable(String name, Map<String, Object> symbolTable) {
        this.name = name;
        this.symbolTable = symbolTable;
    }

    @Override
    public Object execute() {
        // Obtiene el valor de la variable desde la tabla
        return symbolTable.get(name);
    }
}